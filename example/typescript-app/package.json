    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.1",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.1",
        "concurrently": "^9.1.0",
        "nodemon": "^3.1.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "webpack": "^5.97.1",
        "webpack-cli": "^6.0.1",
        "babel-loader": "^9.2.1",
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "^7.26.0"
