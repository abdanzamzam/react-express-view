        "dev": "concurrently \"webpack --watch --mode development\" \"nodemon server.ts\""
    },
    "dependencies": {
        "express": "^4.21.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-express-view": "file:../../"
